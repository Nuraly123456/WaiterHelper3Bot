# –¢–∞–ø—Å—ã—Ä—ã—Å—Ç–∞—Ä–¥—ã —Å–∞“õ—Ç–∞—É (”ô—Ä —Ç–∞–ø—Å—ã—Ä—ã—Å “Ø—à—ñ–Ω –±–∞“ì–∞—Å—ã –¥–∞ —Å–∞“õ—Ç–∞–ª–∞–¥—ã)
orders = []


@dp.message(F.text == "/start")
async def start(message: Message):
    await message.answer(
        "–°”ô–ª–µ–º! –ë“±–ª —Ç–∞–ø—Å—ã—Ä—ã—Å “õ–∞–±—ã–ª–¥–∞—É –±–æ—Ç—ã. –¢–∞–ø—Å—ã—Ä—ã—Å—Ç—ã –∂–∞–π –∂–∞–∑–∞ –±–µ—Ä—ñ“£—ñ–∑, –∞–ª –∫“Ø–Ω —Å–æ“£—ã–Ω–¥–∞ '/close_cash' –∫–æ–º–∞–Ω–¥–∞—Å—ã–Ω –±–∞—Å—ã–ø, –µ—Å–µ–ø—Ç—ñ –∞–ª—ã“£—ã–∑.")


@dp.message(F.text == "/menu")
async def show_menu(message: Message):
    menu_text = "\n".join([f"{item} - {price} —Ç–≥" for item, price in MENU.items()])
    await message.answer(f"üìú –ú–µ–Ω—é:\n{menu_text}")


@dp.message(F.text == "/close_cash")
async def close_cash(message: Message):
    global orders
    if not orders:
        await message.answer("–ë“Ø–≥—ñ–Ω –µ—à“õ–∞–Ω–¥–∞–π —Ç–∞–ø—Å—ã—Ä—ã—Å “õ–∞–±—ã–ª–¥–∞–Ω–±–∞–¥—ã.")
        return

    summary = defaultdict(int)
    total = 0

    # –¢–∞–ø—Å—ã—Ä—ã—Å—Ç–∞—Ä–¥—ã ”©“£–¥–µ—É –∂”ô–Ω–µ –∂–∞–ª–ø—ã —Å–æ–º–∞–Ω—ã –µ—Å–µ–ø—Ç–µ—É
    for order, count in orders:
        summary[order] += count
        total += MENU[order] * count

    summary_text = "\n".join([f"{item}: {count} –¥–∞–Ω–∞" for item, count in summary.items()])
    await message.answer(f"üìä –ö“Ø–Ω–¥—ñ–∫ –µ—Å–µ–ø:\n{summary_text}\nüí∞ –ñ–∞–ª–ø—ã –∫–∞—Å—Å–∞: {total} —Ç–≥")

    # –ö–∞—Å—Å–∞–Ω—ã —Ç–∞–∑–∞—Ä—Ç—É
    orders = []


@dp.message(F.text == "/remove_last")
async def remove_last_order(message: Message):
    if orders:
        removed_order, count = orders.pop()
        await message.answer(f"‚ùå –°–æ“£“ì—ã –µ–Ω–≥—ñ–∑—ñ–ª–≥–µ–Ω —Ç–∞–ø—Å—ã—Ä—ã—Å ”©—à—ñ—Ä—ñ–ª–¥—ñ: {removed_order} x{count}")
    else:
        await message.answer("”®—à—ñ—Ä—É–≥–µ —Ç–∞–ø—Å—ã—Ä—ã—Å –∂–æ“õ!")


@dp.message(F.text == "/clear_orders")
async def clear_orders(message: Message):
    global orders
    orders = []
    await message.answer("üóë –ë–∞—Ä–ª—ã“õ —Ç–∞–ø—Å—ã—Ä—ã—Å—Ç–∞—Ä —Ç–∞–∑–∞—Ä—Ç—ã–ª–¥—ã!")


@dp.message()
async def take_order(message: Message):
    order_text = message.text.strip().lower()  # –¢–∞–ø—Å—ã—Ä—ã—Å—Ç—ã –∫—ñ—à—ñ ”ô—Ä—ñ–ø–∫–µ –∞—É—ã—Å—Ç—ã—Ä—É
    orders_list = order_text.split(",")  # –¢–∞–ø—Å—ã—Ä—ã—Å—Ç–∞—Ä–¥—ã “Ø—Ç—ñ—Ä–º–µ–Ω –±”©–ª—ñ–ø –∞–ª–∞–º—ã–∑

    # –ú”ô–∑—ñ—Ä–¥–µ–≥—ñ –±–∞—Ä–ª—ã“õ –∞—Ç–∞—É–ª–∞—Ä–¥—ã –¥–∞ –∫—ñ—à—ñ ”ô—Ä—ñ–ø–∫–µ –∞—É—ã—Å—Ç—ã—Ä—ã–ø —Å–∞–ª—ã—Å—Ç—ã—Ä—É
    menu_keys = [item.strip().lower() for item in MENU.keys()]

    added_orders = []
    not_found_orders = []

    for order in orders_list:
        order = order.strip()  # ”ò—Ä —Ç–∞–ø—Å—ã—Ä—ã—Å—Ç—ã“£ –∞—Ä–∞—Å—ã–Ω–¥–∞ –∞—Ä—Ç—ã“õ –±–æ—Å –æ—Ä—ã–Ω–¥–∞—Ä–¥—ã –∞–ª—ã–ø —Ç–∞—Å—Ç–∞—É
        matching_orders = [item for item in menu_keys if order in item]

        if matching_orders:
            # “∞“õ—Å–∞—Å —Ç–∞–ø—Å—ã—Ä—ã—Å—Ç–∞—Ä–¥—ã —Ç–∞–ø“õ–∞–Ω–¥–∞, –±—ñ—Ä—ñ–Ω—à—ñ —Ç–∞–±—ã–ª“ì–∞–Ω —Ç–∞–ø—Å—ã—Ä—ã—Å—Ç—ã “õ–æ–ª–¥–∞–Ω–∞–º—ã–∑
            order_name = list(MENU.keys())[menu_keys.index(matching_orders[0])]
            orders.append((order_name, 1))  # –ë—ñ—Ä—ñ–Ω—à—ñ —Ç–∞–ø—Å—ã—Ä—ã—Å —Ä–µ—Ç—ñ–Ω–¥–µ —Å–∞–Ω—ã–Ω 1 –¥–µ–ø –∞–ª–∞–º—ã–∑
            added_orders.append(order_name)
        else:
            not_found_orders.append(order)

    # –ñ–∞—É–∞–ø –±–µ—Ä—É
    if added_orders:
        await message.answer(f"‚úÖ “ö–∞–±—ã–ª–¥–∞–Ω“ì–∞–Ω —Ç–∞–ø—Å—ã—Ä—ã—Å—Ç–∞—Ä: {', '.join(added_orders)}")

    if not_found_orders:
        await message.answer(f"‚ùå –ú”ô–∑—ñ—Ä–¥–µ –∂–æ“õ —Ç–∞–ø—Å—ã—Ä—ã—Å—Ç–∞—Ä: {', '.join(not_found_orders)}")


async def main():
    await dp.start_polling(bot)


if __name__ == "__main__":
    asyncio.run(main())
